{
  "name": "server",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run migrate_prod && npm run build && node dist/src/index.js",
    "dev": "npm run migrate_dev && nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "migrate_prod": "npx prisma migrate deploy",
    "migrate_dev": "npx prisma migrate dev",
    "new-seed": "node create-seed.js"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "nodemon": "^3.1.9",
    "prisma": "^5.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
}
